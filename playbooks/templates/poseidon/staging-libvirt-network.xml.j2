<!-- Virtual network configs for the Poseidon Kubernetes network. -->
<network ipv6='yes'>
  <name>poseidon-k8s-homelab-cm</name>
  <mac address='{{ poseidon_k8s_network_mac_addr }}'/>
  <ip address='{{ poseidon_k8s_network_gateway_ipv4_addr }}' netmask='{{ poseidon_k8s_network_subnet_mask }}'/>
</network>
