---
# {{ ansible_managed }}
apiVersion: operator.tigera.io/v1
kind: Installation
metadata:
  name: default
spec:
  calicoNetwork:
    ipPools:
    - blockSize: {{ k8s_node_prefix }}
      cidr: {{ k8s_pod_subnet }}
      encapsulation: None
      natOutgoing: Enabled
      nodeSelector: all()

---
apiVersion: operator.tigera.io/v1
kind: APIServer 
metadata: 
  name: default 
spec: {}
