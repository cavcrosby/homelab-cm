        "irc_clients": ["irc-client1"],
        "dhcp_servers": ["ansible-controller1"],
        "dns_servers": ["ansible-controller1"],
        "k8s_controllers:vars": {
            "ca_key": "k8s_staging_ca.key",
            "ca_cert": "k8s_staging_ca.crt"
        },
        "all:vars": {
            "ansible_user": "vagrant",
            "ansible_ssh_common_args": "'-o StrictHostKeyChecking=no'"
