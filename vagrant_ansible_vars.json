{
    "ansible_host_vars": {
        "staging-node1": {
            "vagrant_vm_homelab_ipv4_addr": "192.168.1.2",
            "vagrant_vm_mgmt_ipv4_addr": "192.168.3.2",
            "vagrant_vm_vpn_ipv4_addr": "192.168.4.2",
            "vagrant_vm_vpn_wireguard_ipv4_addr": "192.168.4.3",
            "vagrant_vm_homelab_mac_addr": "52:54:00:6d:59:5b",
            "vagrant_vm_mgmt_mac_addr": "52:54:00:8d:44:65",
            "vagrant_vm_vpn_mac_addr": "52:54:00:14:f4:59",
            "vagrant_vm_cpus": 1,
            "vagrant_vm_memory": 512,
            "vagrant_vm_libvirt_disk_size": 10,
            "vagrant_vm_box": "debian/bookworm64",
            "dnsmasq_dhcp_config_file_template": "staging-dnsmasq-dhcp.conf.j2",
            "dnsmasq_dns_config_file_template": "staging-dnsmasq-dns.conf.j2",
            "mgmt_network_interface_name": "en0",
            "homelab_network_interface_name": "en1",
            "vpn_network_interface_name": "en2",
            "wireguard_network_interface_name": "wg0",
            "haproxy_config_file_template": "haproxy.cfg.j2",
            "wikipedia_zim_name": "wikipedia_en_simple_all",
            "wireguard_server_port": 30324,
            "cacerts_bundle_path": "/etc/ssl/certs/ca-certificates.crt",
            "vagrant_config_refs": {
                "dnsmasq_dhcp_listen_ipv4_addr": "vagrant_vm_homelab_ipv4_addr",
                "homelab_dnsmasq_dns_listen_ipv4_addr": "vagrant_vm_homelab_ipv4_addr",
                "vpn_dnsmasq_dns_listen_ipv4_addr": "vagrant_vm_vpn_ipv4_addr",
                "load_balancer_listen_ipv4_addr": "vagrant_vm_homelab_ipv4_addr",
                "systemd_networkd_files": [
                    {
                        "filename": {
                            "join": [
                                "00-",
                                "homelab_network_interface_name",
                                ".link"
                            ]
                        },
                        "Match": {
                            "MACAddress": "vagrant_vm_homelab_mac_addr"
                        },
                        "Link": {
                            "Name": "homelab_network_interface_name"
                        }
                    },
                    {
                        "filename": {
                            "join": [
                                "00-",
                                "homelab_network_interface_name",
                                ".network"
                            ]
                        },
                        "Match": {
                            "Name": "homelab_network_interface_name"
                        },
                        "Network": {
                            "Address": {
                                "join": [
                                    "vagrant_vm_homelab_ipv4_addr",
                                    "/24"
                                ]
                            },
                            "Gateway": "${VAGRANT_LIBVIRT_HOMELAB_NETWORK_IPV4_ADDR}"
                        },
                        "DHCPv4": {
                            "UseHostname": "false"
                        }
                    },
                    {
                        "filename": {
                            "join": [
                                "00-",
                                "mgmt_network_interface_name",
                                ".link"
                            ]
                        },
                        "Match": {
                            "MACAddress": "vagrant_vm_mgmt_mac_addr"
                        },
                        "Link": {
                            "Name": "mgmt_network_interface_name"
                        }
                    },
                    {
                        "filename": {
                            "join": [
                                "00-",
                                "mgmt_network_interface_name",
                                ".network"
                            ]
                        },
                        "Match": {
                            "Name": "mgmt_network_interface_name"
                        },
                        "Network": {
                            "DHCP": "true"
                        },
                        "DHCPv4": {
                            "UseHostname": "false"
                        }
                    },
                    {
                        "filename": {
                            "join": [
                                "00-",
                                "vpn_network_interface_name",
                                ".link"
                            ]
                        },
                        "Match": {
                            "MACAddress": "vagrant_vm_vpn_mac_addr"
                        },
                        "Link": {
                            "Name": "vpn_network_interface_name"
                        }
                    },
                    {
                        "filename": {
                            "join": [
                                "00-",
                                "vpn_network_interface_name",
                                ".network"
                            ]
                        },
                        "Match": {
                            "Name": "vpn_network_interface_name"
                        },
                        "Network": {
                            "Address": {
                                "join": [
                                    "vagrant_vm_vpn_ipv4_addr",
                                    "/24"
                                ]
                            }
                        },
                        "DHCPv4": {
                            "UseHostname": "false"
                        },
                        "Route": {
                            "Gateway": "${VAGRANT_LIBVIRT_VPN_NETWORK_IPV4_ADDR}",
                            "Destination": "0.0.0.0/0",
                            "Metric": "4294967295"
                        }
                    },
                    {
                        "filename": {
                            "join": [
                                "00-",
                                "wireguard_network_interface_name",
                                ".netdev"
                            ]
                        },
                        "NetDev": {
                            "Name": "wireguard_network_interface_name",
                            "Kind": "wireguard"
                        },
                        "WireGuard": {
                            "PrivateKeyFile": {
                                "join": [
                                    "/etc/systemd/network/",
                                    "wireguard_network_interface_name",
                                    ".key"
                                ]
                            },
                            "ListenPort": "wireguard_server_port"
                        },
                        "WireGuardPeer": [
                            {
                                "PublicKey": "${VAGRANT_VPN_CLIENT_1_PUBKEY}",
                                "AllowedIPs": "192.168.4.45/32"
                            },
                            {
                                "PublicKey": "${VAGRANT_VPN_CLIENT_2_PUBKEY}",
                                "AllowedIPs": "192.168.4.46/32"
                            },
                            {
                                "PublicKey": "${VAGRANT_VPN_CLIENT_3_PUBKEY}",
                                "AllowedIPs": "192.168.4.47/32"
                            }
                        ]
                    },
                    {
                        "filename": {
                            "join": [
                                "00-",
                                "wireguard_network_interface_name",
                                ".network"
                            ]
                        },
                        "Match": {
                            "Name": "wireguard_network_interface_name"
                        },
                        "Network": {
                            "Address": {
                                "join": [
                                    "vagrant_vm_vpn_wireguard_ipv4_addr",
                                    "/32"
                                ]
                            }
                        },
                        "Route": [
                            {
                                "Scope": "link",
                                "Source": {
                                    "join": [
                                        "vagrant_vm_vpn_wireguard_ipv4_addr",
                                        "/24"
                                    ]
                                },
                                "Destination": "192.168.4.45/32"
                            },
                            {
                                "Scope": "link",
                                "Source": {
                                    "join": [
                                        "vagrant_vm_vpn_wireguard_ipv4_addr",
                                        "/24"
                                    ]
                                },
                                "Destination": "192.168.4.46/32"
                            },
                            {
                                "Scope": "link",
                                "Source": {
                                    "join": [
                                        "vagrant_vm_vpn_wireguard_ipv4_addr",
                                        "/24"
                                    ]
                                },
                                "Destination": "192.168.4.47/32"
                            }
                        ]
                    }
                ]
            },
            "se_domains": [
                "computergraphics.stackexchange.com"
            ]
        },
        "vmm1": {
            "vagrant_vm_homelab_ipv4_addr": "192.168.1.3",
            "vagrant_vm_mgmt_ipv4_addr": "192.168.3.3",
            "vagrant_vm_vpn_ipv4_addr": "192.168.4.4",
            "vagrant_vm_homelab_mac_addr": "52:54:00:d5:af:7d",
            "vagrant_vm_mgmt_mac_addr": "52:54:00:1b:28:b2",
            "vagrant_vm_vpn_mac_addr": "52:54:00:ad:c7:98",
            "vagrant_vm_cpus": 1,
            "vagrant_vm_memory": 2048,
            "vagrant_vm_libvirt_disk_size": 10,
            "vagrant_vm_box": "debian/bookworm64",
            "mgmt_network_interface_name": "en0",
            "homelab_network_interface_name": "en1",
            "vpn_network_interface_name": "en2",
            "homelab_network_bridge_interface_name": "virbr0",
            "vagrant_config_refs": {
                "systemd_networkd_files": [
                    {
                        "filename": {
                            "join": [
                                "00-",
                                "homelab_network_bridge_interface_name",
                                ".netdev"
                            ]
                        },
                        "NetDev": {
                            "Name": "homelab_network_bridge_interface_name",
                            "Kind": "bridge",
                            "MACAddress": "none"
                        }
                    },
                    {
                        "filename": {
                            "join": [
                                "00-",
                                "homelab_network_bridge_interface_name",
                                ".link"
                            ]
                        },
                        "Match": {
                            "OriginalName": "homelab_network_bridge_interface_name"
                        },
                        "Link": {
                            "MACAddressPolicy": "none"
                        }
                    },
                    {
                        "filename": {
                            "join": [
                                "00-",
                                "homelab_network_bridge_interface_name",
                                ".network"
                            ]
                        },
                        "Match": {
                            "Name": "homelab_network_bridge_interface_name"
                        },
                        "Link": {
                            "RequiredForOnline": "routable"
                        },
                        "Network": {
                            "DHCP": "yes"
                        }
                    },
                    {
                        "filename": {
                            "join": [
                                "00-",
                                "homelab_network_interface_name",
                                ".link"
                            ]
                        },
                        "Match": {
                            "MACAddress": "vagrant_vm_homelab_mac_addr"
                        },
                        "Link": {
                            "Name": "homelab_network_interface_name"
                        }
                    },
                    {
                        "filename": {
                            "join": [
                                "00-",
                                "homelab_network_bridge_interface_name",
                                "-",
                                "homelab_network_interface_name",
                                ".network"
                            ]
                        },
                        "Match": {
                            "Name": "homelab_network_interface_name"
                        },
                        "Network": {
                            "Bridge": "homelab_network_bridge_interface_name"
                        }
                    },
                    {
                        "filename": {
                            "join": [
                                "01-",
                                "mgmt_network_interface_name",
                                ".link"
                            ]
                        },
                        "Match": {
                            "MACAddress": "vagrant_vm_mgmt_mac_addr"
                        },
                        "Link": {
                            "Name": "mgmt_network_interface_name"
                        }
                    },
                    {
                        "filename": {
                            "join": [
                                "01-",
                                "mgmt_network_interface_name",
                                ".network"
                            ]
                        },
                        "Match": {
                            "Name": "mgmt_network_interface_name"
                        },
                        "Network": {
                            "DHCP": "true"
                        },
                        "DHCPv4": {
                            "UseHostname": "false"
                        }
                    },
                    {
                        "filename": {
                            "join": [
                                "00-",
                                "vpn_network_interface_name",
                                ".link"
                            ]
                        },
                        "Match": {
                            "MACAddress": "vagrant_vm_vpn_mac_addr"
                        },
                        "Link": {
                            "Name": "vpn_network_interface_name"
                        }
                    },
                    {
                        "filename": {
                            "join": [
                                "00-",
                                "vpn_network_interface_name",
                                ".network"
                            ]
                        },
                        "Match": {
                            "Name": "vpn_network_interface_name"
                        },
                        "Network": {
                            "Address": {
                                "join": [
                                    "vagrant_vm_vpn_ipv4_addr",
                                    "/24"
                                ]
                            }
                        },
                        "DHCPv4": {
                            "UseHostname": "false"
                        },
                        "Route": {
                            "Gateway": "${VAGRANT_LIBVIRT_VPN_NETWORK_IPV4_ADDR}",
                            "Destination": "0.0.0.0/0",
                            "Metric": "4294967295"
                        }
                    }
                ]
            },
            "lvm_disks_config": {
                "vgs": [
                    {
                        "name": "vgnfs",
                        "pvs": {
                            "disks": [
                                "/dev/vdb"
                            ]
                        }
                    }
                ],
                "lvs": [
                    {
                        "name": "lvzims",
                        "size": "5G",
                        "fstype": "ext4",
                        "vg_name": "vgnfs"
                    },
                    {
                        "name": "lvcache",
                        "fstype": "ext4",
                        "vg_name": "vgnfs"
                    }
                ]
            },
            "nfs_exports_config": [
                {
                    "device": "/srv/nfs/zims",
                    "hosts": [
                        "talos-*(rw,all_squash)"
                    ]
                },
                {
                    "device": "/srv/nfs/cache",
                    "hosts": [
                        "talos-*(rw,all_squash)"
                    ]
                }
            ]
        },
        "irc-client1": {
            "vagrant_vm_homelab_ipv4_addr": "192.168.1.100",
            "vagrant_vm_mgmt_ipv4_addr": "192.168.3.4",
            "vagrant_vm_homelab_mac_addr": "52:54:00:a9:f3:c5",
            "vagrant_vm_mgmt_mac_addr": "52:54:00:7f:5d:e5",
            "vagrant_vm_cpus": 1,
            "vagrant_vm_memory": 512,
            "vagrant_vm_libvirt_disk_size": 10,
            "vagrant_vm_box": "debian/bullseye64"
        }
    },
    "ansible_groups": {
        "on_prem:children": [
            "bare_metal"
        ],
        "irc_clients": [
            "irc-client1"
        ],
        "dhcp_servers": [
            "staging-node1"
        ],
        "dns_servers": [
            "staging-node1"
        ],
        "load_balancers": [
            "staging-node1"
        ],
        "vmms": [
            "vmm1"
        ],
        "nfs_servers": [
            "vmm1"
        ],
        "proxy_servers": [
            "staging-node1"
        ],
        "vpn_servers": [
            "staging-node1"
        ],
        "bare_metal": [
            "staging-node1",
            "vmm1"
        ],
        "all:vars": {
            "ansible_user": "vagrant",
            "ansible_ssh_common_args": "'-o StrictHostKeyChecking=no'"
        }
    }
}
